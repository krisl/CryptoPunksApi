{
  "root": true,
  "env": {
    "es6": true,
    "node": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "globals": {
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "no-console": "off",
    "no-trailing-spaces": "error",
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }]
  }
}
